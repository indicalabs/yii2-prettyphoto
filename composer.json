{
    "name": "indicalabs/yii2-prettyphoto",
    "description": "The Pretty photo widget for the Yii framework",
    "type": "yii2-extension",
    "keywords": ["yii2","extension","prettyphoto","images"],
    "license": "MIT",
    "authors": [
        {
            "name": "Venu Narukulla",
            "email": "venu.narukulla@gmail.com"
        }
    ],
    "require": {
		"yiisoft/yii2": "~2.0.12",
	    "bower-asset/jquery-prettyphoto": "*",
	    "npm-asset/vue-images": ">=1.0.9"
  	},
    
    "require-dev": {
    	"phpunit/phpunit": "~6.2.2"
 	 },
 	 
    "autoload": {
        "psr-4": { 
        	"indicalabs\\prettyphoto\\": "" 
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "bower-asset/jquery-prettyPhoto",
                "type": "bower-asset-library",
                "version": "3.1.6",
                "source": {
                    "url": "https://github.com/scaron/prettyphoto",
                    "type": "git",
                    "reference": "master"
                }
            }
        }
    ]
}
